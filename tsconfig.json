{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES6",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"typeRoots": [
			"./node_modules/@types"
		]
	},
	"exclude": [
		"node_modules"
	]
}